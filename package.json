{
  "name": "vue3-snapshot-serializer",
  "type": "module",
  "version": "0.3.0",
  "description": "Vitest snapshot serializer for Vue 3 components",
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.js src tests",
    "fix": "npm run lint -- --fix",
    "test": "vitest --coverage",
    "debug": "vitest --inspect-brk --no-file-parallelism -t \"Stringify attributes\" \"cheerio\""
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "htmlparser2": "^9.1.0",
    "parse5": "^7.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@stylistic/eslint-plugin-js": "^2.10.1",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/coverage-v8": "^2.1.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.14.0",
    "eslint-config-tjw-base": "^3.0.1",
    "happy-dom": "^15.8.0",
    "vitest": "^2.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjw-lint/vue3-snapshot-serializer.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "vitest",
    "jest",
    "serializer",
    "snapshots",
    "unit",
    "testing",
    "unit",
    "lint"
  ],
  "author": "The Jared Wilcurt",
